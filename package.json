{
  "name": "valentine-wish",
  "version": "1.0.0",
  "description": "A romantic Valentine web page",
  "main": "index.js",
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "gh-pages": "^6.3.0"
  },
"scripts": {
  "start": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js' --port 7777",
  "build": "if exist dist rmdir /s /q dist && mkdir dist && copy index.html dist\\ && copy *.css dist\\ && copy *.js dist\\",
  "predeploy": "npm run build",
  "deploy": "gh-pages -d dist"
},
